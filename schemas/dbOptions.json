{
  "$schema": "https://github.com/Rama-Krishna-P/jsonschemas/blob/main/schemas/dbOptions.json",
  "type": "object",
  "properties": {
    "DbOptions": {
      "type": "object",
      "properties": {
        "DataSource": {
          "type": "string"
        },
        "Port": {
          "type": "integer"
        },
        "Database": {
          "type": "string"
        },
        "Username": {
          "type": "string"
        },
        "Password": {
          "type": "string"
        },
        "AutoMigrate": {
          "type": "boolean"
        }
      },
      "required": [
        "DataSource",
        "Port",
        "Database",
        "Username",
        "Password",
        "AutoMigrate"
      ]
    }
  },
  "required": [
    "DbOptions"
  ]
}
